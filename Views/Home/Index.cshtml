@{
    ViewBag.Title = "Index";
    Layout="~/Views/shared/View2.cshtml";
}
<link href="~/css/bootstrap-theme.css" rel="stylesheet" />
<script src="~/Scripts/jquery-2.1.4.min.js"></script>
<script src="~/Scripts/jquery-2.1.4.intellisense.js"></script>
<script src="~/Scripts/jquery-1.9.1.min.js"></script>
<script>
    jQuery(function () {
        var items = [];
        jQuery('#validate').click(function () {
            jQuery.getJSON("/Account/Validatetheuser", { "name": jQuery('#UserName').val() }, function (successResult) {
                /*if (successResult == true) {
                    alert("Invalid");
                }
                else
                {
                    alert("Valid");
                }*/
                
                $.each(successResult, function (key, val) {
                    //$("#reh").text();
                    items.push('<li id="' + key + '">' + val+ '</li>');
                });
                $('<ul/>', {
                    'class': 'my-new-list',
                    html: items.join('')
                }).appendTo('body');
            });
        });
    });
</script>

<html lang="en">
